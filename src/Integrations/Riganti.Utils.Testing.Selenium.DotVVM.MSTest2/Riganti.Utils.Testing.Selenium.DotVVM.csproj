<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net461;netstandard2.0</TargetFrameworks>
    <IsPackable>true</IsPackable>
    <IncludeSymbols>True</IncludeSymbols>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <AssemblyVersion>2.0.0</AssemblyVersion>
    <FileVersion>2.0.0</FileVersion>
    <Description>Integration of Riganti.Utils.Testing.Selenium.DotVVM for DotVVM Framework.</Description>
    <Version>2.0.0-beta</Version>
    <PackageProjectUrl>https://github.com/riganti/selenium-utils</PackageProjectUrl>
    <RepositoryUrl>https://github.com/riganti/selenium-utils</RepositoryUrl>
    <PackageTags>selenium ui tests riganti ci dotvvm</PackageTags>
    <PackageReleaseNotes>Added support for Selenium.WebDriver.3.6.0</PackageReleaseNotes>
    <Authors>Ladislav Šesták</Authors>
    <Company>RIGANTI s.r.o.</Company>
    <PackageIconUrl>https://github.com/riganti/selenium-utils/raw/master/Riganti.Utils/Icon.png</PackageIconUrl>
